{
  "name": "rc-ui-lib",
  "version": "0.1.49",
  "description": "React Mobile UI Components base on Vant",
  "main": "lib/index.js",
  "module": "es/index.js",
  "style": "lib/index.css",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "es"
  ],
  "scripts": {
    "start": "rc-cli dev",
    "build": "rc-cli build",
    "release": "rc-cli release",
    "build:site": "rc-cli build-site",
    "release:site": "yarn build:site && gh-pages -d site",
    "prepack": "yarn build",
    "test": "rc-cli test --u --colors",
    "test:coverage": "open tests/coverage/index.html"
  },
  "author": "rancui",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:rancui/rc-ui-lib.git"
  },
  "keywords": [
    "ui",
    "react",
    "mobile",
    "components"
  ],
  "dependencies": {
    "@vant/icons": "^1.7.0",
    "@vant/popperjs": "^1.1.0",
    "classnames": "^2.2.6",
    "react-transition-group": "^4.4.1",
    "swiper": "^6.8.1"
  },
  "devDependencies": {
    "@rancui/cli": "^1.0.0",
    "@types/classnames": "^2.2.11",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "gh-pages": "^3.2.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rc-field-form": "^1.21.2"
  },
  "peerDependencies": {
    "react": ">=16.14.0",
    "react-dom": ">=16.14.0",
    "rc-field-form": "^1.21.2"
  },
  "browserslist": [
    "Android >= 4.0",
    "iOS >= 8"
  ],
  "sideEffects": [
    "es/**/style/*",
    "lib/**/style/*",
    "*.css",
    "*.less"
  ]
}
