{
  "name": "rc-ui-lib",
  "version": "0.1.37",
  "description": "React Mobile UI Components base on Vant UI",
  "repository": "https://github.com/rancui/rc-ui-lib",
  "main": "lib/index.js",
  "module": "es/index.js",
  "style": "lib/index.css",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "es"
  ],
  "scripts": {
    "start": "rc-cli dev",
    "build": "rc-cli build",
    "release": "rc-cli release",
    "build:site": "rc-cli build-site",
    "release:site": "yarn build:site && gh-pages -d site",
    "prepack": "yarn build",
    "test": "rc-cli test --u --colors --coverage",
    "test:coverage": "open tests/coverage/index.html"
  },
  "author": "rancui",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "ui",
    "react",
    "mobile",
    "components"
  ],
  "devDependencies": {
    "@rancui/cli": "^1.0.0",
    "@types/classnames": "^2.2.11",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "enzyme-to-json": "^3.6.2",
    "gh-pages": "^3.2.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "peerDependencies": {
    "react": ">=16.14.0",
    "react-dom": ">=16.14.0"
  },
  "browserslist": [
    "Android >= 4.0",
    "iOS >= 8"
  ],
  "dependencies": {
    "@vant/icons": "^1.7.0",
    "@vant/popperjs": "^1.1.0",
    "classnames": "^2.2.6",
    "react-transition-group": "^4.4.1",
    "swiper": "^6.8.1"
  }
}
